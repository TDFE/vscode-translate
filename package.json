{
    "name": "td-vscode-translation",
    "displayName": "td-vscode-translation",
    "description": "同盾vscode翻译",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.67.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:json",
        "onLanguage:javascript",
        "onLanguage:typescript"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js",
        "eslint-fixed": "npx eslint --max-warnings 0 --fix --ext .js,.jsx,.ts,.tsx ./src"
    },
    "devDependencies": {
        "@types/vscode": "^1.67.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "14.x",
        "glob": "^8.0.1",
        "mocha": "^9.2.2",
        "typescript": "^4.6.4",
        "@vscode/test-electron": "^2.1.3",
        "eslint": "^8.16.0",
        "eslint-config-tongdun": "^1.1.10",
        "eslint-plugin-td-rules-plugin": "^1.0.1",
        "lint-staged": "^12.4.2",
        "husky": "^8.0.1"
    },
    "dependencies": {},
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --quiet --fix --ext .js,.jsx,.ts,.tsx"
        ]
    }
}
